<div id="wrapper">
    <%- partial('./../partials/header.ejs') %>
        <!-- /. NAV TOP  -->
        <%- partial('./../partials/sidebar.ejs') %>
            <!-- /. NAV SIDE  -->
            <div id="page-wrapper">
                <table id="dataTable" class="table table-striped table-bordered wrap" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>name</th>
                            <th>phone</th>
                            <th>Appointment Date</th>
                            <th>Created At</th>    
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>#</th>
                            <th>name</th>
                            <th>phone</th>
                            <th>Appointment Date</th>
                            <th>Created At</th>
                        </tr>
                    </tfoot>
                    <tbody>
                        <% for (var i = 0; i < all_users.length; i++) { %>
                            <tr class="linkable-item">
                                <td>
                                    <a href="<%= (all_users[i].service && all_users[i].service.id) ? 'appointment/service' : 'appointment/product' %>/<%=all_users[i].id%>" target="_blank"><button class="btn btn-primary"> View</button></a>
                                </td>
                                <td>
                                    <a href="appointment/customer/<%=(all_users[i].customer && all_users[i].customer.id)%>" target="_blank"><%=all_users[i].customer.name %></a>
                                </td>
                                <td>
                                     <!-- <a href="appointment/phone/<%=all_users[i].customer.phone %>" target="_blank"><%=all_users[i].customer.phone %></a> -->
                                     <%=all_users[i].customer.phone %>
                                </td>
                                <td>
                                    <%= moment.unix(all_users[i].appt_date).format("llll") %>
                                </td>
                                <td>
                                    <%= moment(all_users[i].createdAt).format("llll") %>
                                </td>
                            </tr>
                        <%}%>

                    </tbody>
                </table>
            </div>
            <!-- /. PAGE WRAPPER  -->
</div>
